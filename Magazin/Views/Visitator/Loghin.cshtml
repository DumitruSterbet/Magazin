

@model ViewModel
@{
    ViewData["Title"] = "Logare";
}
<h2>Formularul de logare ca admin</h2>


<form method="post">
    <label>Логин:</label><br />
    <input type="text" name="login" /><br /><br />
    <label>Пароль:</label><br />
    <input type="text" name="password" /><br /><br />
    <input type="submit" value="Отправить" />
</form>

@{foreach (var p in Model.viewAdmin)
    {

        {

            <div class="col-lg-4">
                <h2>@p.login</h2>
                <p>@p.password</p>

            </div>

        }
    }
}
